{
  "name": "nightbff-backend",
  "version": "0.1.0",
  "description": "Backend services for NightBFF nightlife social platform",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "npm run build && node ./node_modules/typeorm/cli.js --dataSource dist/src/data-source.js",
    "typeorm:create-migration": "npm run typeorm -- migration:create src/database/migrations/MigrationName",
    "typeorm:generate-migration": "npm run typeorm -- migration:generate src/database/migrations/NewMigrationName",
    "typeorm:run-migrations": "npm run typeorm -- migration:run",
    "typeorm:revert-migration": "npm run typeorm -- migration:revert"
  },
  "keywords": [
    "nightlife",
    "social",
    "platform",
    "nestjs",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.4.15",
    "@supabase/supabase-js": "^2.0.0",
    "@types/axios": "^0.9.36",
    "@types/ioredis": "^4.28.10",
    "@types/passport-jwt": "^4.0.1",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.4.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^4.1.0",
    "ioredis": "^5.6.0",
    "nestjs-cls": "^3.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.1",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.0",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@testcontainers/postgresql": "^10.24.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.10",
    "testcontainers": "^10.24.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typescript": "^5.1.3"
  }
}
